{
    "name": "ROS 2 Development Container",
    "privileged": true,
    "remoteUser": "user",
    "dockerComposeFile": "./docker-compose.yml",
    "service": "franka_ros2",
    "workspaceFolder": "/ros2_ws/src",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-python.vscode-pylance",
                "ms-python.black-formatter",
                "ms-vscode.cpptools",
                "xaver.clang-format",
                "twxs.cmake",
                "cheshirekow.cmake-format",
                "ms-iot.vscode-ros"
            ]
        }
    }
}
